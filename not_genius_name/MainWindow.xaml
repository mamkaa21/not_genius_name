<Window x:Class="not_genius_name.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:not_genius_name"
         xmlns:w="clr-namespace:not_genius_name.Model"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Grid.Column="1" Grid.Row="4" Content="Добавить паспорт" Click="AddPassport"/>
        <Button Grid.Column="2" Grid.Row="4" Content="Добавить снилс" Click="AddSnils"/>
        <Button Grid.Column="3" Grid.Row="4" Content="Поиск паспорта" Click="SearchPassport"/>
        <Button Grid.Column="4" Grid.Row="4" Content="Поиск снилса" Click="SearchSnils"/>
        <Button Grid.Column="5" Grid.Row="4" Content="Удаление паспорта" Click="DeletePassport"/>
    </StackPanel>
</Window>
