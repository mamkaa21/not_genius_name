<Window x:Class="not_genius_name.DeletePassportWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:not_genius_name"
        mc:Ignorable="d"
        Title="DeletePassportWin" Height="150" Width="300">
    <StackPanel DataContext="{Binding Passport}">
        <Label Content="Серия"/>
        <TextBox Text="{Binding Serial}"/>
        <Label Content="Номер"/>
        <TextBox Text="{Binding Number}"/>
        <Button Content="Удалить" Click="Delete_Post"/>
    </StackPanel>
</Window>
